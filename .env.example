# Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Appwrite Configuration
APPWRITE_ENDPOINT="https://cloud.appwrite.io/v1"
APPWRITE_PROJECT_ID="your-project-id-here"
APPWRITE_API_KEY="your-api-key-here"

# Database IDs (will be generated during setup)
APPWRITE_DATABASE_ID="promptpilot-db"
APPWRITE_WORKFLOWS_COLLECTION_ID="workflows"
APPWRITE_BLOCKS_COLLECTION_ID="blocks"
APPWRITE_EXECUTIONS_COLLECTION_ID="executions"
APPWRITE_EXECUTION_LOGS_COLLECTION_ID="execution-logs"

# Storage Bucket IDs
APPWRITE_GENERATED_FILES_BUCKET_ID="generated-files"
APPWRITE_USER_UPLOADS_BUCKET_ID="user-uploads"

# Function IDs
APPWRITE_AI_TEXT_FUNCTION_ID="ai-text-operations"
APPWRITE_AI_IMAGE_FUNCTION_ID="ai-image-generation" 
APPWRITE_WORKFLOW_EXECUTOR_FUNCTION_ID="workflow-executor"
APPWRITE_WEBHOOK_HANDLER_FUNCTION_ID="webhook-handler"

# External API Keys
OPENAI_API_KEY="your-openai-api-key-here"
STABILITY_AI_API_KEY="your-stability-ai-api-key-here"

# Application Settings
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NEXT_PUBLIC_APPWRITE_ENDPOINT="https://cloud.appwrite.io/v1"
NEXT_PUBLIC_APPWRITE_PROJECT_ID="your-project-id-here"

# Development Settings
NODE_ENV="development"
NEXT_TELEMETRY_DISABLED=1